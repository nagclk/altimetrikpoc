{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adfpochash"
		},
		"LS_Data_Lake_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_Data_Lake'"
		},
		"LS_OnPrem_SQL_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LS_OnPrem_SQL'"
		},
		"databricks_UC_LS_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'databricks_UC_LS'"
		},
		"LS_OnPrem_SQL_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "pocsqlvm"
		},
		"LS_OnPrem_SQL_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "bachempoc"
		},
		"LS_OnPrem_SQL_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "nagAdmin"
		},
		"databricks_UC_LS_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "1230-074831-bze09h20"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adfpochash_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_Data_Lake_connectionString": {
						"value": "[parameters('LS_Data_Lake_connectionString')]"
					},
					"databricks_UC_LS_accessToken": {
						"value": "[parameters('databricks_UC_LS_accessToken')]"
					},
					"LS_OnPrem_SQL_password": {
						"value": "[parameters('LS_OnPrem_SQL_password')]"
					},
					"databricks_UC_LS_properties_typeProperties_existingClusterId": {
						"value": "[parameters('databricks_UC_LS_properties_typeProperties_existingClusterId')]"
					},
					"LS_OnPrem_SQL_properties_typeProperties_server": {
						"value": "[parameters('LS_OnPrem_SQL_properties_typeProperties_server')]"
					},
					"LS_OnPrem_SQL_properties_typeProperties_database": {
						"value": "[parameters('LS_OnPrem_SQL_properties_typeProperties_database')]"
					},
					"LS_OnPrem_SQL_properties_typeProperties_userName": {
						"value": "[parameters('LS_OnPrem_SQL_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}